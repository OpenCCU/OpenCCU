+/*!
+ * Place all our bootstrap related style fixes into this file
+ */
+
+*,
+*::before,
+*::after {
+  box-sizing: revert;
+}
+
+input,
+button,
+select,
+optgroup,
+textarea {
+  margin: revert;
+}
+
+h6, .h6,
+h5, .h5,
+h4, .h4,
+h3, .h3,
+h2, .h2,
+h1, .h1 {
+  margin-top: revert;
+}
+
+#header,
+.UIFrameContent {
+  line-height: initial;
+}
+
+.FooterButton {
+  box-sizing: content-box;
+}
+
+#header table,
+#content table,
+#messagebox table {
+  border-collapse: separate;
+}
+
+#footer table {
+  border-collapse: inherit;
+}
+
+.popupTable thead,
+.popupTable tbody,
+.popupTable tfoot,
+.popupTable tr,
+.popupTable td,
+.popupTable th {
+  border-style: inherit;
+  border-width: inherit;
+}
